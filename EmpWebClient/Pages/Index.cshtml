@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}


<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Name </th>
            <th scope="col">Age </th>
            <th scope="col">Salary </th>
            <th scope="col">Dept </th>
    </thead>
    <tbody>
        @foreach (var emp in Model.Employees)
        {
            <tr>

                <td>@emp.Name </td>
                <td>@emp.Age </td>
                <td>@emp.Salary </td>
                <td>@emp.Dept </td>
            </tr>
        }

    </tbody>
</table>